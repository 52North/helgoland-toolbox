{
  "extends": "../../tslint.json",
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      "n52",
      "camelCase"
    ],
    "no-unused-expression": false,
    "component-selector": [
      true,
      "element",
      "n52",
      "kebab-case"
    ]
  },
  "linterOptions": {
    "exclude": [
      "!**/*"
    ]
  }
}