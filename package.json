    "@types/geojson": "^7946.0.4",
    "@types/jquery": "^3.3.6",
    "@types/leaflet": "1.2.13",
    "@types/leaflet.markercluster": "^1.0.3",
    "@types/moment": "^2.13.0",
    "@types/plotly.js": "^1.38.6",
    "bootstrap-slider": "^9.9.0",
    "class-transformer": "^0.1.9",
