{
  "name": "helgoland-toolbox",
  "version": "8.0.3",
  "description": "Helgoland toolbox",
  "license": "Apache-2.0",
  "keywords": [
    "helgoland",
    "angular",
    "webpack",
    "typescript"
  ],
  "author": {
    "name": "Jan Schulte"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/52North/helgoland-toolbox.git"
  },
  "bugs": {
    "url": "https://github.com/52North/helgoland-toolbox/issues"
  },
  "homepage": "https://github.com/52North/helgoland-toolbox#readme",
  "scripts": {
    "affected": "nx affected",
    "affected:apps": "nx affected:apps",
    "affected:build": "nx affected:build",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected:e2e": "nx affected:e2e",
    "affected:libs": "nx affected:libs",
    "affected:lint": "nx affected:lint",
    "affected:test": "nx affected:test",
    "build": "ng build",
    "compodoc": "./node_modules/.bin/compodoc libs -p tsconfig.doc.json --includes docs",
    "dep-graph": "nx dep-graph",
    "e2e": "ng e2e",
    "gh-pages": "rimraf documentation && npm run compodoc && gh-pages -d documentation",
    "lib:build": "./node_modules/ts-node/dist/bin.js --skip-project scripts/build.ts",
    "lib:pack": "./node_modules/ts-node/dist/bin.js --skip-project scripts/pack.ts",
    "lib:bumpVersion": "./node_modules/ts-node/dist/bin.js --skip-project scripts/bumpVersions.ts",
    "lib:publish": "./node_modules/ts-node/dist/bin.js --skip-project scripts/publish.ts",
    "format": "nx format:write",
    "format:check": "nx format:check",
    "format:write": "nx format:write",
    "help": "nx help",
    "lint": "nx workspace-lint && ng lint",
    "ng": "ng",
    "nx": "nx",
    "preversion": "ng lint && npm test",
    "version": "npm run lib:bumpVersion && npm run lib:build && git add -A .",
    "postversion": "git push && git push --tags && npm run lib:publish",
    "start": "ng serve",
    "test": "rimraf dist && ng test",
    "update": "ng update @nrwl/workspace",
    "workspace-schematic": "nx workspace-schematic"
  },
  "private": true,
