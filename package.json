{
  "name": "helgoland-toolbox",
  "version": "10.0.0-beta.1",
  "description": "Helgoland toolbox",
  "license": "Apache-2.0",
  "keywords": [
    "helgoland",
    "angular",
    "webpack",
    "typescript"
  ],
  "author": {
    "name": "Jan Schulte"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/52North/helgoland-toolbox.git"
  },
  "bugs": {
    "url": "https://github.com/52North/helgoland-toolbox/issues"
  },
  "homepage": "https://github.com/52North/helgoland-toolbox#readme",
  "scripts": {
    "affected": "nx affected",
    "affected:apps": "nx affected:apps",
    "affected:build": "nx affected:build",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected:e2e": "nx affected:e2e",
    "affected:libs": "nx affected:libs",
    "affected:lint": "nx affected:lint",
    "affected:test": "nx affected:test",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "compodoc": "./node_modules/.bin/compodoc libs -p tsconfig.doc.json --includes docs",
    "dep-graph": "nx dep-graph",
    "e2e": "ng e2e",
    "gh-pages": "rimraf documentation && npm run compodoc && gh-pages -d documentation",
    "lib:build": "./node_modules/ts-node/dist/bin.js --skip-project scripts/build.ts",
    "lib:pack": "./node_modules/ts-node/dist/bin.js --skip-project scripts/pack.ts",
    "lib:bumpVersion": "./node_modules/ts-node/dist/bin.js --skip-project scripts/bumpVersions.ts",
    "lib:publish": "./node_modules/ts-node/dist/bin.js --skip-project scripts/publish.ts",
    "format": "nx format:write",
    "format:check": "nx format:check",
    "format:write": "nx format:write",
    "help": "nx help",
    "lint": "nx workspace-lint && ng lint",
    "ng": "ng",
    "nx": "nx",
    "preversion": "ng lint && npm test",
    "version": "npm run lib:bumpVersion && npm run lib:build && git add -A .",
    "postversion": "git push && git push --tags && npm run lib:publish",
    "start": "ng serve",
    "prestart": "npm run versions-script",
    "test": "rimraf dist && ng test",
    "update": "ng update @nrwl/workspace",
    "workspace-schematic": "nx workspace-schematic",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "versions-script": "node scripts/git.version.js",
    "i18n:extract": "ngx-translate-extract --input ./apps/helgoland/src --output ./apps/helgoland/src/assets/i18n/{en,de}.json --clean --format --sort json"
  },
  "private": true,
  "dependencies": {
    "@angular-material-components/color-picker": "^4.0.2",
    "@angular/animations": "^10.0.0",
    "@angular/cdk": "^10.2.2",
    "@angular/common": "^10.0.0",
    "@angular/compiler": "^10.0.0",
    "@angular/core": "^10.0.0",
    "@angular/forms": "^10.0.0",
    "@angular/material": "^10.2.2",
    "@angular/material-moment-adapter": "^10.2.5",
    "@angular/platform-browser": "^10.0.0",
    "@angular/platform-browser-dynamic": "^10.0.0",
    "@angular/router": "^10.0.0",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "@nrwl/angular": "^10.2.1",
    "angular-plotly.js": "^3.0.0",
    "bootstrap-slider": "^11.0.2",
    "class-transformer": "^0.3.1",
    "d3": "^5.12.0",
    "d3-delaunay": "^5.2.1",
    "font-awesome": "^4.7.0",
    "jquery": "^3.5.1",
    "leaflet": "^1.7.1",
    "leaflet.markercluster": "^1.4.1",
    "moment": "^2.28.0",
    "moment-timezone": "^0.5.31",
    "ngx-clipboard": "^13.0.1",
    "ngx-color-picker": "^10.0.1",
    "ol": "^6.4.3",
    "plotly.js": "^1.54.5",
    "resize-observer-polyfill": "^1.5.1",
    "rxjs": "^6.6.3",
    "tslib": "^2.0.0",
    "typescript-logging": "^0.6.3",
    "xlsx": "^0.16.7",
    "zone.js": "^0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1001.2",
    "@angular-devkit/build-ng-packagr": "^0.1001.2",
    "@angular/cli": "^10.1.7",
    "@angular/compiler-cli": "^10.0.0",
    "@angular/language-service": "^10.0.0",
    "@biesbjerg/ngx-translate-extract": "^7.0.3",
    "@compodoc/compodoc": "^1.1.11",
    "@nrwl/cypress": "^10.2.1",
    "@nrwl/jest": "^10.2.1",
    "@nrwl/nx-cloud": "latest",
    "@nrwl/workspace": "^10.2.1",
    "@types/d3": "^5.7.2",
    "@types/d3-delaunay": "^4.1.0",
    "@types/geojson": "^7946.0.7",
    "@types/jest": "^26.0.14",
    "@types/leaflet": "^1.5.17",
    "@types/leaflet.markercluster": "^1.4.2",
    "@types/moment": "^2.13.0",
    "@types/moment-timezone": "^0.5.30",
    "@types/node": "~12.7.12",
    "@types/ol": "^5.3.5",
    "@types/plotly.js": "^1.50.19",
    "codelyzer": "~5.0.1",
    "cypress": "^4.1.0",
    "dotenv": "6.2.0",
    "eslint": "6.8.0",
    "jest": "26.2.2",
    "jest-canvas-mock": "^2.2.0",
    "jest-preset-angular": "8.2.1",
    "ng-packagr": "^10.0.0",
    "prettier": "2.0.4",
    "ts-jest": "26.1.4",
    "ts-node": "~7.0.0",
    "tslint": "~6.0.0",
    "typescript": "~3.9.3"
  }
}
