{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "allowSyntheticDefaultImports": true,
    "lib": [
      "es2015",
      "dom"
    ],
    "typeRoots": [
      "node_modules/@types"
    ],
    "types": [
      "geojson",
      "hammerjs",
      "jasmine",
      "node",
      "source-map",
      "uglify-js",
      "webpack",
      "jquery",
      "flot"
    ],
    "baseUrl": "./src",
    "paths": {
      "@helgoland/core": [
        "@helgoland/core/src/core.ts"
      ],
      "@helgoland/core/src/*": [
        "@helgoland/core/src/*"
      ],
      "@helgoland/caching": [
        "@helgoland/caching/src/caching.ts"
      ],
      "@helgoland/caching/src/*": [
        "@helgoland/caching/src/*"
      ],
      "@helgoland/control": [
        "@helgoland/control/src/control.ts"
      ],
      "@helgoland/control/src/*": [
        "@helgoland/control/src/*"
      ],
      "@helgoland/depiction": [
        "@helgoland/depiction/src/depiction.ts"
      ],
      "@helgoland/depiction/label-mapper": [
        "@helgoland/depiction/label-mapper/src/label-mapper"
      ],
      "@helgoland/depiction/label-mapper/src/*": [
        "@helgoland/depiction/label-mapper/src/*"
      ],
      "@helgoland/depiction/dataset-table": [
        "@helgoland/depiction/dataset-table/src/dataset-table"
      ],
      "@helgoland/depiction/dataset-table/src/*": [
        "@helgoland/depiction/dataset-table/src/*"
      ],
      "@helgoland/depiction/datasetlist": [
        "@helgoland/depiction/datasetlist/src/datasetlist"
      ],
      "@helgoland/depiction/datasetlist/src/*": [
        "@helgoland/depiction/datasetlist/src/*"
      ],
      "@helgoland/depiction/src/*": [
        "@helgoland/depiction/src/*"
      ],
      "@helgoland/favorite": [
        "@helgoland/favorite/src/favorite.ts"
      ],
      "@helgoland/favorite/src/*": [
        "@helgoland/favorite/src/*"
      ],
      "@helgoland/flot": [
        "@helgoland/flot/src/flot.ts"
      ],
      "@helgoland/flot/src/*": [
        "@helgoland/flot/src/*"
      ],
      "@helgoland/map": [
        "@helgoland/map/src/map.ts"
      ],
      "@helgoland/map/control": [
        "@helgoland/map/control/src/control"
      ],
      "@helgoland/map/control/src/*": [
        "@helgoland/map/control/src/*"
      ],
      "@helgoland/map/selector": [
        "@helgoland/map/selector/src/selector"
      ],
      "@helgoland/map/selector/src/*": [
        "@helgoland/map/selector/src/*"
      ],
      "@helgoland/map/view": [
        "@helgoland/map/view/src/view"
      ],
      "@helgoland/map/view/src/*": [
        "@helgoland/map/view/src/*"
      ],
      "@helgoland/map/src/*": [
        "@helgoland/map/src/*"
      ],
      "@helgoland/permalink": [
        "@helgoland/permalink/src/permalink.ts"
      ],
      "@helgoland/permalink/src/*": [
        "@helgoland/permalink/src/*"
      ],
      "@helgoland/plotly": [
        "@helgoland/plotly/src/plotly.ts"
      ],
      "@helgoland/plotly/src/*": [
        "@helgoland/plotly/src/*"
      ],
      "@helgoland/selector": [
        "@helgoland/selector/src/selector.ts"
      ],
      "@helgoland/selector/src/*": [
        "@helgoland/selector/src/*"
      ],
      "@helgoland/time": [
        "@helgoland/time/src/time.ts"
      ],
      "@helgoland/time/src/*": [
        "@helgoland/time/src/*"
      ],
      "@helgoland/modification": [
        "@helgoland/modification/src/modification.ts"
      ],
      "@helgoland/modification/src/*": [
        "@helgoland/modification/src/*"
      ],
      "@helgoland/d3": [
        "@helgoland/d3/src/d3.ts"
      ],
      "@helgoland/d3/src/*": [
        "@helgoland/d3/src/*"
      ]
    }
  },
  "exclude": [
    "node_modules",
    "dist",
    "dist_package",
    "e2e",
    "src/**/*.spec.ts"
  ],
  "compileOnSave": false,
  "buildOnSave": false,
  "atom": {
    "rewriteTsconfig": false
  }
}
