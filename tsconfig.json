        "dist/helgoland/time-range-slider"
      ],
      "@helgoland/time-range-slider/*": [
        "dist/helgoland/time-range-slider/*"
      ],
      "@helgoland/auth": [
        "dist/helgoland/auth"
      ],
      "@helgoland/auth/*": [
        "dist/helgoland/auth/*"
      ],
      "@helgoland/eventing": [
        "dist/helgoland/eventing"
      ],
      "@helgoland/eventing/*": [
        "dist/helgoland/eventing/*"
      ]
