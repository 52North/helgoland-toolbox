        "dist/helgoland/core"
      ],
      "@helgoland/core/*": [
        "dist/helgoland/core/*"
      ],
      "@helgoland/map": [
        "dist/helgoland/map"
      ],
      "@helgoland/map/*": [
        "dist/helgoland/map/*"
      ],
      "@helgoland/caching": [
        "dist/helgoland/caching"
      ],
      "@helgoland/caching/*": [
        "dist/helgoland/caching/*"
      ],
      "@helgoland/control": [
        "dist/helgoland/control"
      ],
      "@helgoland/control/*": [
        "dist/helgoland/control/*"
      ],
      "@helgoland/depiction": [
        "dist/helgoland/depiction"
      ],
      "@helgoland/depiction/*": [
        "dist/helgoland/depiction/*"
      ],
      "@helgoland/favorite": [
        "dist/helgoland/favorite"
      ],
